<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="autostopBundle\Entity\Estudiante" table="estudiante">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sexo" type="boolean" column="sexo" nullable="false"/>
    <field name="nombre" type="string" column="nombre" length="20" nullable="false"/>
    <field name="apellido" type="string" column="apellido" length="20" nullable="false"/>
    <field name="usuario" type="string" column="usuario" length="15" nullable="false"/>
    <field name="contrasena" type="string" column="contrasena" length="20" nullable="false"/>
    <field name="email" type="string" column="email" length="50" nullable="false"/>
    <field name="foto" type="string" column="foto" length="50" nullable="true"/>
    <field name="visitas" type="integer" column="visitas" nullable="true"/>
    <field name="estado" type="boolean" column="estado" nullable="true"/>
    <field name="latitud" type="float" column="latitud" precision="10" scale="0" nullable="true"/>
    <field name="longitud" type="float" column="longitud" precision="10" scale="0" nullable="true"/>
  </entity>
</doctrine-mapping>
